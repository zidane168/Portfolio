{
    "name": "chickenpot",
    "version": "0.0.1",
    "description": "Chickenpot Ecommerce",
    "author": "Vi Ly Huu",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
      "prebuild": "rimraf dist",
      "clean": "rimraf logs/*/ public/uploads/_tmp/*",
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
      "start": "nest start",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "cli:dev": "ts-node -r tsconfig-paths/register ./src/cli.ts",
      "cli": "node dist/cli",
      "db:generate": "prisma generate",
      "db:deploy": "prisma migrate deploy",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
      "@casl/ability": "^6.5.0",
      "@casl/prisma": "^1.4.1",
      "@faker-js/faker": "^8.4.0",
      "@nestjs/axios": "^3.0.0",
      "@nestjs/bull": "^10.0.0",
      "@nestjs/cache-manager": "^2.0.1",
      "@nestjs/common": "^10.0.0",
      "@nestjs/config": "^3.0.0",
      "@nestjs/core": "^10.0.0",
      "@nestjs/jwt": "^10.1.0",
      "@nestjs/mapped-types": "*",
      "@nestjs/passport": "^10.0.0",
      "@nestjs/platform-express": "^10.0.0",
      "@nestjs/platform-socket.io": "^10.0.5",
      "@nestjs/schedule": "^3.0.1",
      "@nestjs/serve-static": "^4.0.0",
      "@nestjs/swagger": "^7.1.1",
      "@nestjs/terminus": "^10.0.1",
      "@nestjs/throttler": "^5.0.0",
      "@nestjs/websockets": "^10.0.5",
      "@prisma/client": "^5.3.1",
      "@types/multer": "^1.4.7",
      "@types/uuid": "^9.0.2",
      "axios": "^1.4.0",
      "bcrypt": "^5.1.0",
      "bull": "^4.10.4",
      "cache-manager": "^5.2.3",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.0",
      "crypto-js": "^4.1.1",
      "firebase-admin": "^11.11.0",
      "helmet": "^7.0.0",
      "image-size": "^1.0.2",
      "moment-timezone": "^0.5.43",
      "morgan": "^1.10.0",
      "ms": "^2.1.3",
      "multer": "^1.4.5-lts.1",
      "nest-commander": "^3.11.1",
      "nest-winston": "^1.9.3",
      "nestjs-firebase": "^10.0.0",
      "nestjs-i18n": "^10.2.6",
      "nodemailer": "^6.9.3",
      "passport": "^0.6.0",
      "passport-headerapikey": "^1.2.2",
      "passport-jwt": "^4.0.1",
      "range-parser": "^1.2.1",
      "reflect-metadata": "^0.1.13",
      "response-time": "^2.3.2",
      "rimraf": "^5.0.1",
      "rotating-file-stream": "^3.1.1",
      "rxjs": "^7.8.1",
      "socket.io": "^4.7.1",
      "ua-parser-js": "^1.0.35",
      "winston": "^3.10.0",
      "winston-daily-rotate-file": "^4.7.1",
      "xlsx": "^0.18.5"
    },
    "devDependencies": {
      "@nestjs/schematics": "^10.0.1",
      "@nestjs/testing": "^10.0.0",
